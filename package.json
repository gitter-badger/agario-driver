  "license": "ISC",
  "bugs": {
    "url": "https://github.com/scaljeri/agario/issues"
  },
  "homepage": "https://github.com/scaljeri/agario#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "mocha": "^2.4.5",
    "mocktail": "^0.3.8",
    "numeric": "^1.2.6",
    "promise": "^7.1.1",
    "victor": "^1.1.0",
    "watchify": "^3.7.0"
  }
